{
    "name": "ugi-rankings",
    "version": "1.0.0",
    "description": "UGI rankings system",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "docker:build": "docker build -t ugi-rankings .",
        "docker:run": "docker-compose up -d",
        "docker:stop": "docker-compose down",
        "test": "echo \"Error: no test specified\" && exit 1"
    },
    "keywords": [
        "ugi",
        "rankings",
        "chess",
        "games"
    ],
    "author": "JamesMHarmon",
    "license": "MIT",
    "dependencies": {
        "@tensorflow/tfjs-node": "^4.0.0",
        "pg": "^8.11.0",
        "commander": "^11.0.0",
        "dotenv": "^16.0.0"
    },
    "devDependencies": {
        "typescript": "^5.0.0",
        "ts-node": "^10.9.0",
        "ts-node-dev": "^2.0.0",
        "@types/node": "^20.0.0",
        "@types/pg": "^8.10.0",
        "nodemon": "^3.0.0"
    }
}